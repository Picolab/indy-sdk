{
  "name": "indy-sdk",
  "version": "0.0.0",
  "private": true,
  "description": "node bindings for libindy",
  "main": "index.js",
  "dependencies": {
    "bindings": "^1.2.1",
    "node-addon-api": "^0.3.5"
  },
  "devDependencies": {
    "blocked": "^1.2.1",
    "mocha": "^3.5.0",
    "node-gyp": "^3.6.2"
  },
  "scripts": {
    "install": "node-gyp rebuild",
    "test": "mocha --napi-modules test",
    "macos-errno": "less /usr/include/sys/errno.h",
    "libhash": "cd src/hash; make test && make && make clean",
    "libindy-symbols": "nm include/libindy.a | grep ' T _indy_' | awk '{print $3}'"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "author": "Ant Cosentino <ant@io.co.za>",
  "license": "MIT",
  "gypfile": true
}
